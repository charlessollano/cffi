    "@sequelize/core": "^7.0.0-alpha.10",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.0",
    "bootstrap": "4.4.1",
    "chart.js": "3.4.1",
    "chroma-js": "2.4.2",
    "classnames": "^2.3.2",
    "global": "^4.4.0",
    "headroom.js": "^0.12.0",
    "mariadb": "^3.0.2",
    "moment": "^2.29.4",
    "nouislider": "^15.6.1",
    "prop-types": "15.8.1",
    "react": ">=16.8.0",
    "react-chartjs-2": "3.0.4",
    "react-countup": "6.2.0",
